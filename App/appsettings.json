{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": {
      "Dev": "Host=postgres;Port=5432;Database=postgres;Username=postgres;Password=postgres;Pooling=true;Include Error Detail=true"
    },
    "Redis": {
      "Dev": "cache:6379"
    }
  },
  "Jwt": {
    "Secret": "personal-long-secret-key-jwt-certification",
    "ExpirationInMinutes": 60,
    "Issuer": "Issuer", // 토큰 발급자
    "Audience": "user" // 토큰 대상자
  }
}
